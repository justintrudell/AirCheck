doctype html
html(lang="en")
  head
      link(href='bootstrap.css', rel='stylesheet')
      link(href='bootstrap.min.css', rel='stylesheet')
      link(href='style.css', rel='stylesheet')
      link(href='font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')
  body
  div(align='center')
    strong
    h1(style='font-weight:bold;') Symptoms Datasheet
    br
    br
    form(align='center', action='symptomsentry', method='get')
        h2(align='center') Which city are you located in?
        br
        input(type='text', name='city', id='city')
        button(id='submitCity', type='button') Find me!
        br
        br
        h4 or
        br
        br
        div(align='center')
            button(id='locationButton', type='button') Get your current location
        br
        br
        h4 Longitude:
            input(id='longitude', disabled)
        br
        br
        h4 Latitude:
            input(id='latitude', disabled)
        br
        br
        div(id='map')
        br
        br
        h2(align='center') Fill out your symptoms below from 1 to 10:
        br
        span(padding-right='20')
        h4 Cough level:
            select(name='coughLevel')
                option(value='0') 0
                option(value='1') 1
                option(value='2') 2
                option(value='3') 3
                option(value='4') 4
                option(value='5') 5
                option(value='6') 6
                option(value='7') 7
                option(value='8') 8
                option(value='9') 9
                option(value='10') 10
        br
        h4 Shortness of breath:
            select(name='howIsBreath')
                option(value='0') 0
                option(value='1') 1
                option(value='2') 2
                option(value='3') 3
                option(value='4') 4
                option(value='5') 5
                option(value='6') 6
                option(value='7') 7
                option(value='8') 8
                option(value='9') 9
                option(value='10') 10
        br
        h4 Wheezing:
            select(name='wheezing')
                option(value='0') 0
                option(value='1') 1
                option(value='2') 2
                option(value='3') 3
                option(value='4') 4
                option(value='5') 5
                option(value='6') 6
                option(value='7') 7
                option(value='8') 8
                option(value='9') 9
                option(value='10') 10
        br
        h4 Sneezing:
            select(name='sneezing')
                option(value='0') 0
                option(value='1') 1
                option(value='2') 2
                option(value='3') 3
                option(value='4') 4
                option(value='5') 5
                option(value='6') 6
                option(value='7') 7
                option(value='8') 8
                option(value='9') 9
                option(value='10') 10
        br
        h4 Is your nose obstructed?
            select(name='noseBlock')
                        option(value='1') Yes
                        option(value='0') No
        br
        h4 Are your eyes itchy?
            select(name='itchyEyes')
                        option(value='1') Yes
                        option(value='0') No
        br
        br
        br
        input(id='submitButton', type='submit')
        br
        br
        br
        br

    script(type='text/javascript', src='https://code.jquery.com/jquery-git.min.js')
    script(type='text/javascript', src='aircheckscript.js')
    script(type='text/javascript', src='gmaps.js')
    script(type='text/javascript', src='http://maps.google.com/maps/api/js')