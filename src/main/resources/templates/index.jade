    link(href='style.css', rel='stylesheet')

  body#top
    div(class='se-pre-con')
    // Header
    header#header.skel-layers-fixed
      h1
        a Spaceapps 2016
      nav#nav
        ul
          li
            a#home Home
          li
            a.heatmap Map
          li
            a.form Report
          li
            a.twit Twitter
    // Banner
    section#banner
      .inner
        h2 This is AirCheck
        p
          | Allergies explained.
        ul.actions
          li
            a.button.big.special.heatmap View map
          li
            a.button.big.form Report symptoms
          li
            a.button.big(id='generateData') Generate User Data
    // One
    section#one.wrapper.style1
      header.major
        h2 Disease heatmap
      .container
        .row
          .12u
            section.special.box.status-wrapper
              #air-quality-status-wrapper
                h4.air-quality Air Quality:
                #air-quality-status.air-quality
              div(id='map')
        div(class='toggle')
          a.button(id='toggle_symptoms', class='toggle-btn') Toggle Symptoms
          a.button(id='toggle_temp', class='toggle-btn') Toggle Temperature
    // Two
    section#two.wrapper.style2
      header.major
        h2 Tell us how you're feeling
        p Help us crowdsource allergy and disease data
      .container
        .12u
          section.special
            iframe#my_typeform(src='https://jerry117.typeform.com/to/UlrIrF')
    // Three
    section#three.wrapper.style3
      header.major
        h2 Twitter updates
        p Tweet @aircheckappsto to report symptoms
      .container
        .12u
          section.special
            a.twitter-timeline(href='https://twitter.com/search?q=%40AircheckAppsTO', data-widget-id='724235280958545920') Tweets about @AircheckAppsTO
            script.
              !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+"://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");
            a.twitter-timeline(href='https://twitter.com/AircheckAppsTO', data-widget-id='724236152153231360') Tweets by @AircheckAppsTO
            script.
              !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+"://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");

